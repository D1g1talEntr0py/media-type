{
  "name": "@d1g1tal/mime-type",
  "description": "Parses, serializes, and manipulates MIME types, according to the WHATWG MIME Sniffing Standard",
  "version": "3.0.3",
  "author": "Domenic Denicola <d@domenic.me> (https://domenic.me/)",
	"maintainers": [{
		"name": "Jason DiMeo",
    "email": "jason.dimeo@gmail.com"
	}],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D1g1talEntr0py/mime-type.git"
  },
	"publishConfig": {
    "access": "public"
  },
	"type": "module",
	"exports": {
		".": {
			"import": "./index.js",
			"default": "./dist/mime-type.min.js" 
		}
	},
	"browser": "./dist/mime-type.min.js",
  "files": [ "index.js", "/src", "dist/" ],
	"keywords": [
    "content-type",
    "mime type",
    "mimesniff",
    "http",
    "whatwg"
  ],
  "scripts": {
		"build": "rimraf dist && esbuild src/mime-type.js --bundle --platform=browser --global-name=MIMEType --footer:js='window.MIMEType = MIMEType.default;' --outfile=dist/mime-type.js && esbuild src/mime-type.js --bundle --sourcemap --minify --platform=browser --global-name=MIMEType --footer:js='window.MIMEType = MIMEType.default;' --outfile=dist/mime-type.min.js",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "coverage": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "pretest": "node scripts/get-latest-platform-tests.js"
  },
  "devDependencies": {
    "esbuild": "^0.15.10",
    "eslint": "^8.24.0",
    "eslint-plugin-compat": "^4.0.2",
    "jest": "^29.1.2",
    "minipass-fetch": "^2.1.2",
    "printable-string": "^0.3.0",
    "rimraf": "^3.0.2",
    "whatwg-encoding": "^2.0.0"
  },
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "Firefox ESR",
    "not dead",
    "not ios_saf < 15",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ]
  }
}
